<div class="profile background-style">

  <%= @user.name %>
  <%= @user.email %>

  <% if @user == current_user %>
    <%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %> |

    <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>

  <% end %>
</div>